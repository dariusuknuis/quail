{
    "files.associations": {
        "*.log": "ini",
    },
    "go.testEnvVars": {
        "SINGLE_TEST": "1"
    },
    "go.testEnvFile": "${workspaceFolder}/.env",
    "workbench.editorAssociations": {
        "*.hex": "hexEditor.hexedit",
        "*.eqg": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.s3d": "hexEditor.hexedit",
        "*.mod": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.mds": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.ter": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.zon": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.lay": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.pts": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.prt": "hexEditor.hexedit", //"binary-file-viewer.viewer",
        "*.pprof": "easyview.view",

    },
    "files.trimTrailingWhitespace": true,
    "binary-file-viewer.parserFolders": [
        ".vscode/parsers"
    ],
    "search.exclude": {
        "**/.git": true,
        "**/test_data": true,
    },
    "window.title": "quail"
}